{
    // "extends": "@tsconfig/node16/tsconfig.json",
    "compilerOptions": {
        "strict": true,
        "target": "es2015",
        "module": "commonjs",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true
    },
    // 指定哪些文件需要编译
    // "files": [
    //   "demo.ts"
    // ]
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules",
        "**/*.spec.ts"
    ]
}
